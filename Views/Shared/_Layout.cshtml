<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="content-type" content="text/html;charset=UTF-8">
    <meta charset="utf-8">
    <title>Play Sistemas - @ViewBag.Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta content="" name="description">
    <meta content="" name="author">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <environment include="Development">
        <!-- Begin Imports do TemplateAdmin -->
        <link rel="shortcut icon" href="~/images/favicon.png" type="image/x-icon"><!-- Favicon -->
        <!-- CORE CSS FRAMEWORK - START -->
        <link href="~/Functions/Script/pace/pace-theme-flash.css" rel="stylesheet" />
        <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
        <link href="~/lib/bootstrap/dist/css/bootstrap-theme.min.css" rel="stylesheet" type="text/css" />
        <!--<link href="~/css/font-awesome.css" rel="stylesheet" type="text/css" />-->
        <link href="~/css/PainelGestor/Content/font-awesome-4.7.0/css/font-awesome.css" rel="stylesheet" />
        <link href="~/css/animate.min.css" rel="stylesheet" type="text/css" />
        <link href="~/Functions/Script/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" type="text/css" />
        <!-- CORE CSS FRAMEWORK - END -->
        <link href="~/css/responsive.css" rel="stylesheet" />
        <!-- End Imports do TemplateAdmin -->

        <link rel="stylesheet" href="~/css/site.css" />
        <link href="~/css/dynamiccss.css" rel="stylesheet" />
        <link href="~/css/global.css" rel="stylesheet" />
        <link href="~/css/style.css" rel="stylesheet" />

    </environment>
    <environment exclude="Development">
        <!-- Begin Imports do TemplateAdmin -->
        <link rel="shortcut icon" href="~/images/favicon.png" type="image/x-icon"><!-- Favicon -->
        <!-- CORE CSS FRAMEWORK - START -->
        <link href="~/Functions/Script/pace/pace-theme-flash.css" rel="stylesheet" />
        <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
        <link href="~/lib/bootstrap/dist/css/bootstrap-theme.min.css" rel="stylesheet" type="text/css" />
        <!--<link href="~/css/font-awesome.css" rel="stylesheet" type="text/css" />-->
        <link href="~/css/PainelGestor/Content/font-awesome-4.7.0/css/font-awesome.css" rel="stylesheet" />
        <link href="~/css/animate.min.css" rel="stylesheet" type="text/css" />
        <link href="~/Functions/Script/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" type="text/css" />
        <!-- CORE CSS FRAMEWORK - END -->
        <link href="~/css/responsive.css" rel="stylesheet" />
        <!-- End Imports do TemplateAdmin -->

        <link rel="stylesheet" href="~/css/site.css" />
        <link href="~/css/dynamiccss.css" rel="stylesheet" />
        <link href="~/css/global.css" rel="stylesheet" />
        <link href="~/css/style.css" rel="stylesheet" />
    </environment>
    @RenderSection("Styles", required: false)
</head>
<body>
    <div class="menu">
        <ul class="menu-options">
            <li class="menu-option" id="ver_log"><a onclick=""></a>Ver logs</li>
            <li class="menu-option" id="ver_log_registro"><a onclick=""></a>Ver logs do registro</li>
        </ul>
    </div>
    <div data-toggle="modal" data-target="modal_sessao"></div>
    <div data-toggle="modal" data-target="modal_alert"></div>

    <div class="modal fade" id="modal_sessao" role="dialog" data-backdrop="static">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title">Sessão expirada</h4>
                </div>
                <div class="modal-body">
                    <p>Clique para fazer log in no sistema novamente</p>
                </div>
                <div class="modal-footer">
                    <a asp-area="" asp-controller="Acesso" asp-action="Logoff">
                        <i class="fa fa-sign-in"></i>
                        Login
                    </a>
                </div>
            </div>
        </div>

    </div>

    <div class="modal fade" id="modal_alert" role="dialog" data-backdrop="static">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title">Sua sessão expira em 2 minutos</h4>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Fechar</button>
                </div>
            </div>
        </div>

    </div>
    <!-- Begin TopBar -->
    <div class="page-topbar">
        <a href="/DynamicWeb/Index" id="logo-marca-link">
            <div class="logo-area" id="logo-area-inicio">
            </div>
        </a>
        <div class="quick-area">
            <div class="pull-left">
                <ul class="info-menu left-links list-inline list-unstyled">
                    <li class="sidebar-toggle-wrap">
                        <a href="#" data-toggle="sidebar" class="sidebar_toggle">
                            <i class="fa fa-bars"></i>
                        </a>
                    </li>
                </ul>
            </div>
            <!--begin notifications--->
            <div class='pull-left'>
                <ul class="info-menu left-links list-inline list-unstyled">
                    <li class="sidebar-toggle-wrap">
                        <div id="notificaoes_1">


                        </div>

                    </li>
                </ul>
            </div>
            <div class='pull-left'>
                <ul class="info-menu left-links list-inline list-unstyled">
                    <li class="sidebar-toggle-wrap">
                        <div id="notificaoes_2"></div>

                    </li>
                </ul>
            </div>
            <div class='pull-left'>
                <ul class="info-menu left-links list-inline list-unstyled">
                    <li class="sidebar-toggle-wrap">
                        <div id="notificaoes_3"></div>
                    </li>
                </ul>
            </div>
            <!--end notifications-->

            <div class="pull-right">
                <ul class="info-menu right-links list-inline list-unstyled">
                    <li class="profile showopacity">
                        <a href="#" data-toggle="dropdown" class="toggle">
                            <img src="~/images/profile.jpg" alt="user-image" class="img-circle img-inline">
                            <span>@ViewBag.UserName&nbsp;<i class="fa fa-angle-down"></i></span>
                        </a>
                        <ul class="dropdown-menu profile animated fadeIn">
                            @*<li>
                                <a href="#">
                                    <i class="fa fa-wrench"></i>
                                    Settings
                                </a>
                            </li>*@
                            <li>
                                <a href="#">
                                    <i class="fa fa-user" onclick="mostrarUsuarioLogado()"></i>
                                    Profile
                                </a>
                            </li>
                            <li>
                                <a asp-area="" asp-controller="DynamicWeb" asp-action="VersaoSistema">
                                    <i class="fa fa-info"></i>
                                    Versão
                                </a>
                            </li>
                            <li class="last">
                                <a asp-area="" asp-controller="Acesso" asp-action="Logoff">
                                    <i class="fa fa-lock"></i>
                                    Logout
                                </a>
                            </li>
                        </ul>
                    </li>
                    <li class="chat-toggle-wrapper">
                        <a href="#" data-toggle="chatbar" class="toggle_chat">
                            @*<i class="fa fa-comments"></i>
                                <span class="badge badge-accent">9</span>
                                <i class="fa fa-times"></i>*@
                        </a>
                    </li>
                </ul>
            </div>
        </div>

    </div>
    <!-- End TopBar -->
    <!-- Begin SideBar -->
    <div class="page-sidebar fixedscroll">

        <!-- MAIN MENU - START -->
        <div class="page-sidebar-wrapper ps-container ps-active-y" id="main-menu-wrapper" style="height: 220px;">

            <!-- USER INFO - START -->
            <div class="profile-info row">

                <div class="profile-image col-xs-4">
                    <a href="#">
                        <img alt="" src="~/images/profile.jpg" class="img-responsive img-circle">
                    </a>
                </div>

                <div class="profile-details col-xs-8">
                    <h3>
                        <a href="#">@ViewBag.UserName</a>
                        <!-- Available statuses: online, idle, busy, away and offline -->
                        <span class="profile-status online"></span>
                    </h3>
                    <p class="profile-title">Seja bem vindo!</p>
                </div>

            </div>
            <!-- USER INFO - END -->

            <ul class="wraplist" style="height: auto;">

                @await Component.InvokeAsync("Menus")

            </ul>

            <div class="ps-scrollbar-x-rail" style="left: 0px; bottom: 3px;"><div class="ps-scrollbar-x" style="left: 0px; width: 0px;"></div></div><div class="ps-scrollbar-y-rail" style="top: 0px; height: 220px; right: 3px;"><div class="ps-scrollbar-y" style="top: 0px; height: 19px;"></div></div>
        </div>
        <!-- MAIN MENU - END -->
    </div>
    <!-- End SideBar -->

    <partial name="_CookieConsentPartial" />
    <!-- Begin Area Content -->
    <section id="main-content" class="">
        <section class="wrapper main-wrapper row" style="">
            <div class="exibir">
           -     @RenderBody()
            </div>
        </section>
    </section>
    <!-- End Area Content -->

    <environment include="Development">
        <!-- initially hidden right-click menu -->

        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
        <script src="~/Functions/Script/play.js"></script>
        <script src="~/Functions/Script/control-carregando.js"></script>
        <script src="~/Functions/Script/DynamicForm.js"></script>
        <script src="~/Functions/Script/OtimizadorCarga/OtimizadorCarga.js"></script>
        <script src="~/Functions/Script/mover-coluna-tabela.js"></script>
        <script src="~/Functions/Script/global.js"></script>
        <script src="~/jquery-mask-plugin/src/jquery.mask.js"></script>
        <!-- Begin Imports do TemplateAdmin -->
        <script src="~/js/jquery-1.11.2.min.js"></script>
        <script>var $j1112 = jQuery.noConflict(true);</script>
        <script src="~/js/jquery.easing.min.js" type="text/javascript"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>
        @*<script src="~/Functions/Script/pace/pace.min.js" type="text/javascript"></script>*@
        <script src="~/Functions/Script/perfect-scrollbar/perfect-scrollbar.js"></script>
        <script src="~/Functions/Script/viewport/viewportchecker.js" type="text/javascript"></script>
        <script src="~/Functions/Script/scripts.js"></script>
        <!-- End Imports do TemplateAdmin -->

        <script>

            // Tratamento de erros
            $(document).ajaxError(function (event, jqxhr, settings, thrownError) {
                realizarTratamentoDeErros(event, jqxhr, settings, thrownError);
            });

        </script>

    </environment>
    <environment exclude="Development">
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
        <script src="~/Functions/Script/play.js"></script>
        <script src="~/Functions/Script/control-carregando.js"></script>
        <script src="~/Functions/Script/DynamicForm.js"></script>
        <script src="~/Functions/Script/OtimizadorCarga/OtimizadorCarga.js"></script>
        <script src="~/Functions/Script/mover-coluna-tabela.js"></script>
        <script src="~/Functions/Script/global.js"></script>
        <script src="~/Functions/Script/Qualidade/DynamicModal.js"></script>

        <!-- Begin Imports do TemplateAdmin -->
        <script src="~/js/jquery-1.11.2.min.js"></script>
        <script>var $j1112 = jQuery.noConflict(true);</script>
        <script src="~/js/jquery.easing.min.js" type="text/javascript"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>
        @*<script src="~/Functions/Script/pace/pace.min.js" type="text/javascript"></script>*@
        <script src="~/Functions/Script/perfect-scrollbar/perfect-scrollbar.js"></script>
        <script src="~/Functions/Script/viewport/viewportchecker.js" type="text/javascript"></script>
        <script src="~/Functions/Script/scripts.js"></script>
        <!-- End Imports do TemplateAdmin -->

        <script>

            // Tratamento de erros
            $(document).ajaxError(function (event, jqxhr, settings, thrownError) {
                realizarTratamentoDeErros(event, jqxhr, settings, thrownError);
            });

        </script>

    </environment>
    @RenderSection("Scripts", required: false)


</body>
</html>
