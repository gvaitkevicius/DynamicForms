@{
    Layout = null;
    dynamic listaEtiquetas = ViewData["ListaEtiquetas"] as System.Collections.Generic.List<string>;
    dynamic dados = ViewData["dados"] as System.Collections.Generic.List<System.Dynamic.ExpandoObject>;
}

<!DOCTYPE html>
<html>
<head>

    <style>
        th,
        td {
            border: 2px solid black;
            border-collapse: collapse;
            padding: 5px;
            text-align: left;
        }

        .tabela {
            border: 2px solid black;
            border-collapse: collapse;
        }

        .element_first {
            width: 15%;
            background-color: blue;
        }

        .element_last {
            width: 15%;
            background-color: red;
        }

        .element_midle {
            background-color: green;
            width: 50%;
        }

        #corpo {
            width: 100%;
            height: 100vh;
            display: flex;
            align-items: center;
            justify-content: center;
        }

        h4 {
            margin: 0;
            padding: 0;
            margin-top: -5px;
        }

        p {
            margin: 0;
        }

        .centralizar {
            color: black;
            text-align: center;
            justify-content: center;
            width: 100%;
        }
        /*Primeira tabela*/

        .primeiraTabelaTd {
            height: 55px;
        }

        /*Segunda tabela*/
        .segundaTabelaTd {
            height: 45px;
        }

        .segundaTabelaH4 {
            margin-top: -5px;
        }

        .TabelaH4 {
            margin-top: auto;
        }

        segundaTabelaP {
            margin-top: 35px;
        }
        .noprint {
            display: none
        }

        .print {
            display: block !important;
        }

        .TipoProduto {
            font-size:50px;
            font-family: 'Times New Roman';
        }
    </style>
</head>

<body id="corpo">
    @{
        int count = 0;
    }


    @foreach (dynamic etiqueta in listaEtiquetas)
    {
        <div>--------------------------------------------------------------------------------------------------------------------------------------------------------------------------</div>
        <div class="centralizar">
            <div style=" width:10%; height:150px; float:left; margin-top:50px;">
            </div>
            <div style=" width:80%; height:200px;float:left;"><img width="600" height="200" src="~/images/logoParaibuna.png"></div>
            <div style=" width:10%; height:150px; float:right; margin-top:50px;">

            </div>
        </div>

        @Html.Raw(etiqueta);


        @if (++count < listaEtiquetas.Count)
        {
            <p style="page-break-after: always;"></p>
        }

    }
</body>

</html>